root = true

[*]
insert_final_newline = true
indent_style = space
trim_trailing_whitespace = true
ij_html_space_inside_empty_tag = true
ij_xml_space_inside_empty_tag = true

[*.feature]
end_of_line = lf
indent_size = 2

[*.{js,json,yml,ts}]
end_of_line = lf
indent_size = 2

[*.cs]
end_of_line = lf
indent_size = 4

[*.{xml,config,props,vue,html,htm,csproj}]
end_of_line = lf
indent_size = 2

